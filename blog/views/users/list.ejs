<!-- 引入头部 -->
<% include haen%>
  <div class="col-md-12">
    <ol class="breadcrumb">
      <li><a href="#">项目管理</a></li>
      <li class="active">添加项目</li>
      <li class="active">项目列表</li>
    </ol>
  </div>
  <h2>人员名单</h2>
  <ul class="nun">
    <li class="iiii">
      <span class="oo">名称</span>
      <span class="oo">描述</span>
      <span class="oo">操作</span>
    </li>
   <% title.forEach(function(arr){%>
    <li class="iiii">
      <span class="oo"><%=arr.name%></span>
      <span class="oo"><%=arr.describe%></span>
      <span>
        <a href="/users/editor/<%=arr._id%>">编辑</a>
      </span>
      <span>
      <a href="/users/delete/<%=arr._id%>" onclick="return confirm('您确定要删除该人员的信息吗')">删除</a> 
      </span>
    </li>
    <%})%>
  </ul>
